{
  "name": "页面接口",
  "version": "1.0.0",
  "description": "页面接口API",
  "group": "xiang/page",
  "guard": "in-process",
  "paths": [
    {
      "path": "/:name/data",
      "method": "GET",
      "process": "xiang.page.Data",
      "in": ["$param.name", ":query"],
      "out": {
        "status": 200,
        "type": "application/json"
      }
    },
    {
      "path": "/:name/setting",
      "method": "GET",
      "process": "xiang.page.Setting",
      "in": ["$param.name", "$query.select"],
      "out": {
        "status": 200,
        "type": "application/json"
      }
    }
  ]
}
