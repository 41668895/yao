{
  "name": "数据分析接口",
  "version": "1.0.0",
  "description": "数据分析接口API",
  "group": "xiang/chart",
  "guard": "in-process",
  "paths": [
    {
      "path": "/:name/data",
      "method": "GET",
      "process": "xiang.chart.Data",
      "in": ["$param.name", ":query"],
      "out": {
        "status": 200,
        "type": "application/json"
      }
    },
    {
      "path": "/:name/setting",
      "method": "GET",
      "process": "xiang.chart.Setting",
      "in": ["$param.name", "$query.select"],
      "out": {
        "status": 200,
        "type": "application/json"
      }
    }
  ]
}
