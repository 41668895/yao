{
  "name": "系统接口",
  "version": "1.0.0",
  "description": "系统接口API",
  "group": "sys",
  "guard": "bearer-jwt",
  "paths": [
    {
      "path": "/info/:id",
      "method": "GET",
      "process": "models.user.Find",
