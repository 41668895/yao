{
  "label": "选择管理者",
  "version": "1.0.0",
  "description": "选择管理者",
  "nodes": [
    {
      "name": "负责人",
      "engine": "xiang",
      "query": {
        "select": ["id"],
        "from": "$xiang.user",
        "wheres": [{ ":type": "类型", "=": "staff" }],
        "first": true
      },
      "orders": [":RAND()"]
    }
  ],
  "output": "{{$res.负责人.id}}"
}
